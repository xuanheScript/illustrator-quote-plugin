<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="12.0" ExtensionBundleId="com.illustrator.annotation.panel" ExtensionBundleVersion="2.0.0" ExtensionBundleName="Illustrator Annotation Panel">
  <Author>
    <![CDATA[Illustrator Annotation Plugin Team]]>
  </Author>
  <Contact>
    <![CDATA[https://github.com/yourusername/illustrator-annotation-plugin]]>
  </Contact>
  <Legal>
    <![CDATA[MIT License]]>
  </Legal>
  <Abstract>
    <![CDATA[Adobe Illustrator材质标注插件 - CEP 12版本，兼容Illustrator 2025+]]>
  </Abstract>
  <ExtensionList>
    <Extension Id="com.illustrator.annotation.panel" Version="2.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="[29.0,99.9]" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="12.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.illustrator.annotation.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <ScriptPath>./jsx/applyMaterial.jsx</ScriptPath>
          <ScriptPath>./jsx/exportQuote.jsx</ScriptPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Illustrator Annotation (CEP 12)</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>350</Width>
            </Size>
            <MinSize>
              <Height>400</Height>
              <Width>300</Width>
            </MinSize>
            <MaxSize>
              <Height>800</Height>
              <Width>500</Width>
            </MaxSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./icons/icon.png</Icon>
            <Icon Type="RollOver">./icons/icon.png</Icon>
            <Icon Type="DarkNormal">./icons/icon.png</Icon>
            <Icon Type="DarkRollOver">./icons/icon.png</Icon>
          </Icons>
        </UI>
        <ExtensionData>
        </ExtensionData>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
